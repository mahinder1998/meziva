{%- comment -%}
  Reusable testimonial card
  Usage:
  {% render 'testimonial-card', testimonial: block.settings %}
{%- endcomment -%}

{%- assign t = testimonial | default: t | default: settings -%}

<article class="sw-h-full sw-rounded-[20px] sw-border sw-border-[#A6A6A6] sw-bg-[#1e1e1e] sw-px-4 sw-py-4 xl:sw-px-6 xl:sw-py-5 sw-flex sw-flex-col sw-gap-3
xl:sw-p-6
">
  {%- comment -%} Top row: avatar + name + meta {%- endcomment -%}
  <div class="sw-flex sw-items-center sw-gap-3">
    {% if t.avatar != blank %}
      <div class="sw-w-14 sw-h-14 sw-rounded-full sw-overflow-hidden sw-flex-shrink-0">
        <img
          src="{{ t.avatar | img_url: '120x120' }}"
          alt="{{ t.name | escape }}"
          class="sw-w-full sw-h-full sw-object-cover"
        >
      </div>
    {% endif %}
    <div class="sw-flex-1 sw-space-y-1">
      {% if t.name != blank %}
        <div class="sw-text-[14px] sw-font-body xl:sw-text-[24px] sw-font-semibold">
          {{ t.name }}
        </div>
      {% endif %}
      {% if t.subtitle != blank %}
        <div class="sw-text-[12px] xl:sw-text-[16px] sw-text-[#D1D5DB]">
          {{ t.subtitle }}
        </div>
      {% endif %}
      {% if t.time_meta != blank %}
        <div class="sw-text-[12px] sw-text-[#9CA3AF] xl:sw-text-[14px] sw-mt-0.5 sw-flex sw-items-center sw-gap-2">
